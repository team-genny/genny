{
  "name": "genny",
  "version": "1.0.0",
  "description": "A RESTful API and web GUI for generating dummy data against a schema.",
  "main": "build/api/server.js",
  "scripts": {
    "test": "npm run api:test",
    "build": "npm run api:build && npm run fe:build",
    "start": "npm run api:start",
    "dev": "concurrently \" npm run api:dev \" \" npm run fe:dev \"",
    "postinstall": "cd src/frontend && npm install",
    "api:test": "jest",
    "api:build": "tsc --build --clean && tsc",
    "api:start": "node build/api/server.js",
    "api:dev": "tsx ./src/api/server.ts",
    "fe:init": "cd src/frontend && npm install",
    "fe:build": "cd src/frontend && npm run build",
    "fe:dev": "cd src/frontend && npm run dev",
    "api:lint": "eslint src/api/**/*.ts",
    "fe:lint": "eslint src/frontend/**/*.ts"
  },
  "keywords": [],
  "contributors": [
    { "name": "Fergus Bentley", "email": "mail@fergcb.uk", "url": "https://fergcb.uk" },
    { "name": "Aamirah Patel", "email": "aamirahpatel@hotmail.co.uk", "url": "https://github.com/AamirahP" },
    { "name": "Joshua Mushaikwa", "email": "jjnotm@gmail.com", "url": "https://github.com/incogzino" },
    { "name": "Nial", "email": "nial@n1al.com", "url": "https://n1al.com" },
    { "name": "sanju", "email": "ghandhisanju@gmail.com", "url": "https://n1al.com" }
  ],
  "license": "ISC",
  "dependencies": {
    "@eslint/js": "^9.1.1",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.1",
    "express": "^4.19.2",
    "express-async-handler": "^1.2.0",
    "mongodb": "^6.5.0",
    "mongoose": "^8.3.1",
    "typescript-eslint": "^7.7.1",
    "uuid": "^9.0.1",
    "winston": "^3.13.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/uuid": "^9.0.8",
    "concurrently": "^8.2.2",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "tsx": "^4.7.2",
    "typescript": "^5.4.5"
  }
}
